<?xml version='1.0' encoding='ISO-8859-1'?>
<!-- The root node is the meta-model -->
<Model Name="doublePendulum">
  <!-- List of connected sub-models -->
  <SubModels>
    <SubModel StartCommand="StartTLMFmiWrapper1" ExactStep="0" ModelFile="doublePendulum1TLM.fmu" Name="doublePendulum1">
      <!-- TLM interface points for SubModel A -->
      <InterfacePoint Name="tlm"/>
    </SubModel>
    <SubModel StartCommand="StartTLMOpenModelica" ExactStep="0" ModelFile="doublePendulum2.mo" Name="doublePendulum2">
      <Annotation Visible="true" Rotation="0" Extent="{-10,-10,10,10}" Origin="{-44,20}"/>
      <!-- TLM interface points for SubModel C -->
      <InterfacePoint Name="tlm"/>
    </SubModel>
  </SubModels>
  <!-- List of TLM connections -->
  <Connections>
    <!-- Some more physical values: Delay="1e-5" Zf="8e4" Zfr="400" alpha="0.2" -->
    <Connection From="doublePendulum1.tlm" alpha="0.9" Zf="2000" To="doublePendulum2.tlm" Delay="2e-4" Zfr="1">
      <Annotation Points="{{11,8},{9,8},{9,28},{-33,28}}"/>
    </Connection>
  </Connections>
  <!-- Parameters for the simulation -->
  <SimulationParams ManagerPort="11113" StartTime="0" StopTime="3"/>
</Model>
