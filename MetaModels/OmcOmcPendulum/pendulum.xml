      <!-- positioned (0,0,0) inside external-model -->
      <InterfacePoint Name="tlm" Domain="Signal" Type="SignalInput" Position="0,-3.0581e-013,0" Angle321="0,0,0"/>
    </SubModel>
  </SubModels>
  <!-- List of TLM connections -->
  <Connections>
    <!-- Some more physical values: Delay="1e-5" Zf="8e4" Zfr="400" alpha="0.2" -->
    <Connection Delay="1e-4" alpha="0.2" Zfr="100" To="shaft2.tlm" Zf="10000" From="shaft1.tlm">
      <Annotation Points="{{12,-2},{20,-2},{20,38},{-18,38},{-18,38}}"/>
    </Connection>
  </Connections>
  <!-- Parameters for the simulation -->
  <SimulationParams StopTime="1" StartTime="0"/>
</Model>
