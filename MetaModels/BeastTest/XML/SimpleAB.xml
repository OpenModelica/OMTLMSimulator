<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- The root node is the meta-model -->
<Model Name="SimpleAB">

    <!-- List of connected sub-models -->
    <SubModels>	
        <SubModel Name="TLMRing_9"
                StartCommand="./StartTLMBeast1" 
                ExactStep="0">
            <!-- TLM interface points for SubModel A -->
                <InterfacePoint Name="ct1:ctlf1_TLM1"/>
        </SubModel>
        <SubModel Name="TLMRing3000_9" 
                StartCommand="./StartTLMBeast" 
                ExactStep="0">
            <!-- TLM interface points for SubModel B -->
                <InterfacePoint Name="ct1:ctlf1_TLM1"/>
        </SubModel>
    </SubModels>

    <!-- List of TLM connections -->
    <Connections>
        <Connection From="TLMRing_9.ct1:ctlf1_TLM1" To="TLMRing3000_9.ct1:ctlf1_TLM1" 
                Delay="0.001" Zf="10" alpha="1e-6"/>
    </Connections>

    <!-- Parameters for the simulation -->
    <SimulationParams ManagerPort="11111" 
                StartTime="0" StopTime="0.01"/>

</Model>
