    <SubModel Position="0.0,0.0,0.0" Name="shaft1" ModelFile="shaft1.mo" GeometryFile="shaft1.stl" Angle321="0.0,0.0,0.0" ExactStep="0" StartCommand="StartTLMOpenModelica">
