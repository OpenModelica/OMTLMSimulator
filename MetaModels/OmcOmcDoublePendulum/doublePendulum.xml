<?xml version='1.0' encoding='ISO-8859-1'?>
<!-- The root node is the meta-model -->
<Model Name="doublePendulum">
  <!-- List of connected sub-models -->
  <SubModels>
    <SubModel ExactStep="1" Name="doublePendulum1" ModelFile="doublePendulum1.mo" StartCommand="StartTLMOpenModelica">
      <!-- TLM interface points for SubModel A -->
      <InterfacePoint Name="tlm"/>
    </SubModel>
    <SubModel ExactStep="1" Name="doublePendulum2" ModelFile="doublePendulum2.mo" StartCommand="StartTLMOpenModelica">
      <!-- TLM interface points for SubModel C -->
      <InterfacePoint Name="tlm"/>
    </SubModel>
  </SubModels>
  <!-- List of TLM connections -->
  <Connections>
    <!-- Some more physical values: Delay="1e-5" Zf="8e4" Zfr="400" alpha="0.2" -->
    <Connection Zfr="1" From="doublePendulum1.tlm" alpha="0.9" To="doublePendulum2.tlm" Zf="2000" Delay="2e-4"/>
  </Connections>
  <!-- Parameters for the simulation -->
  <SimulationParams StartTime="0" StopTime="3" ManagerPort="11113"/>
</Model>
