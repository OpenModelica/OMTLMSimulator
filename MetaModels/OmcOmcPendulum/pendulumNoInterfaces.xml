<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- The root node is the meta-model -->
<Model Name="Pendulum">

    <!-- List of connected external-models -->
    <SubModels>	

        <!-- external model "shaft1" positioned at (0,0,0) inside meta-model  -->
        <SubModel Name="shaft1"
                  StartCommand="StartTLMOpenModelica" 
                  ExactStep="0"
                  ModelFile="shaft1.mo"
                  Position="0.0,0.0,0.0"
		  Angle321="0.0,0.0,0.0">
        <Annotation Origin="{30,10}" Extent="{-10,-10,10,10}" Rotation="0" Visible="true"/>

        </SubModel>

        <!-- external model "shaft2" positioned at (0,0,0.5) inside meta-model -->
        <SubModel Name="shaft2" 
                  StartCommand="StartTLMOpenModelica" 
                  ExactStep="0"
                  ModelFile="shaft2.mo"
		  Position="0.0,0.0,0.5"
		  Angle321="0.0,0.0,0.0">
        <Annotation Origin="{-30,30}" Extent="{-10,-10,10,10}" Rotation="0" Visible="true"/>

        </SubModel>

    </SubModels>
    
    <Connections/>

    <!-- Parameters for the simulation -->
    <SimulationParams  
                StartTime="0" 
                StopTime="5"/>

</Model>
